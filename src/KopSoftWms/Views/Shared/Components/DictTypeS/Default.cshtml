@model List<KeyValuePair<int, string>>

<select class="form-control" v-model="DictType" style="width:150px;margin-left:10px;">
    <option value="">全部类型</option>
    @{
        if (Model.Any())
        {
            foreach (var item in Model)
            {
                <option value="@item.Key">@item.Value</option>
            }
        }
    }
</select>
